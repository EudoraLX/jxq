    <!-- 软删除：将del_flag设置为'1' -->
    <update id="deleteErpMoldById" parameterType="Long">
        update erp_mold set del_flag = '1' where id = #{id} and del_flag = '0'
    </update>

    <!-- 批量软删除：将del_flag设置为'1' -->
    <update id="deleteErpMoldByIds" parameterType="String">
        update erp_mold set del_flag = '1' where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
        and del_flag = '0'
    </update>

    <!-- 物理删除：真正从数据库中删除记录 -->
    <delete id="physicalDeleteErpMoldById" parameterType="Long">
        delete from erp_mold where id = #{id}
    </delete>

