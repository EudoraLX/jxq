    <sql id="selectErpMoldVo">
        select id, mold_code, mold_name, mold_type, mold_category, product_material_id, storage_location_id, mold_weight, mold_size, cavity_count, mold_down_time, mold_up_time,
               max_shot_weight, min_shot_weight, design_life, current_usage_count, maintenance_cycle, last_maintenance_date, next_maintenance_date, mold_status, supplier_id, purchase_date, purchase_price, is_active, remark, create_by, create_time, update_by, update_time, del_flag from erp_mold
    </sql>

    <select id="selectErpMoldList" parameterType="ErpMold" resultMap="ErpMoldResult">
        <include refid="selectErpMoldVo"/>
        <where>  
            and del_flag = '0'
            <if test="moldCode != null  and moldCode != ''"> and mold_code = #{moldCode}</if>
            <if test="moldName != null  and moldName != ''"> and mold_name like concat('%', #{moldName}, '%')</if>
            <if test="moldType != null  and moldType != ''"> and mold_type = #{moldType}</if>
            <if test="moldCategory != null  and moldCategory != ''"> and mold_category = #{moldCategory}</if>
            <if test="moldWeight != null "> and mold_weight = #{moldWeight}</if>
            <if test="moldSize != null  and moldSize != ''"> and mold_size = #{moldSize}</if>
            <if test="cavityCount != null "> and cavity_count = #{cavityCount}</if>
            <if test="moldStatus != null  and moldStatus != ''"> and mold_status = #{moldStatus}</if>
            <if test="isActive != null  and isActive != ''"> and is_active = #{isActive}</if>
        </where>
    </select>
    
