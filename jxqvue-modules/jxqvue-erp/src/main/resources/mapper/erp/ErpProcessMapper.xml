<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jxqvue.erp.mapper.ErpProcessMapper">
    
    <resultMap type="ErpProcess" id="ErpProcessResult">
        <result property="id"    column="id"    />
        <result property="processCode"    column="process_code"    />
        <result property="processName"    column="process_name"    />
        <result property="processType"    column="process_type"    />
        <result property="standardTime"    column="standard_time"    />
        <result property="setupTime"    column="setup_time"    />
        <result property="teardownTime"    column="teardown_time"    />
        <result property="status"    column="status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
        <result property="delFlag"    column="del_flag"    />
    </resultMap>

    <sql id="selectErpProcessVo">
        select id, process_code, process_name, process_type, standard_time, setup_time, teardown_time, status, create_by, create_time, update_by, update_time, remark, del_flag from erp_process
    </sql>

    <select id="selectErpProcessList" parameterType="ErpProcess" resultMap="ErpProcessResult">
        <include refid="selectErpProcessVo"/>
        <where>  
            and del_flag = '0'
            <if test="processCode != null  and processCode != ''"> and process_code = #{processCode}</if>
            <if test="processName != null  and processName != ''"> and process_name like concat('%', #{processName}, '%')</if>
            <if test="processType != null  and processType != ''"> and process_type = #{processType}</if>
            <if test="standardTime != null "> and standard_time = #{standardTime}</if>
            <if test="setupTime != null "> and setup_time = #{setupTime}</if>
            <if test="teardownTime != null "> and teardown_time = #{teardownTime}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectErpProcessById" parameterType="Long" resultMap="ErpProcessResult">
        <include refid="selectErpProcessVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertErpProcess" parameterType="ErpProcess" useGeneratedKeys="true" keyProperty="id">
        insert into erp_process
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="processCode != null and processCode != ''">process_code,</if>
            <if test="processName != null and processName != ''">process_name,</if>
            <if test="processType != null">process_type,</if>
            <if test="standardTime != null">standard_time,</if>
            <if test="setupTime != null">setup_time,</if>
            <if test="teardownTime != null">teardown_time,</if>
            <if test="status != null">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
            <if test="delFlag != null">del_flag,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="processCode != null and processCode != ''">#{processCode},</if>
            <if test="processName != null and processName != ''">#{processName},</if>
            <if test="processType != null">#{processType},</if>
            <if test="standardTime != null">#{standardTime},</if>
            <if test="setupTime != null">#{setupTime},</if>
            <if test="teardownTime != null">#{teardownTime},</if>
            <if test="status != null">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
            <if test="delFlag != null">#{delFlag},</if>
         </trim>
    </insert>

    <update id="updateErpProcess" parameterType="ErpProcess">
        update erp_process
        <trim prefix="SET" suffixOverrides=",">
            <if test="processCode != null and processCode != ''">process_code = #{processCode},</if>
            <if test="processName != null and processName != ''">process_name = #{processName},</if>
            <if test="processType != null">process_type = #{processType},</if>
            <if test="standardTime != null">standard_time = #{standardTime},</if>
            <if test="setupTime != null">setup_time = #{setupTime},</if>
            <if test="teardownTime != null">teardown_time = #{teardownTime},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteErpProcessById" parameterType="Long">
        update erp_process set del_flag = '1' where id = #{id}
    </delete>

    <delete id="deleteErpProcessByIds" parameterType="String">
        update erp_process set del_flag = '1' where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
